+ * card-background-color:
+ * card-text-color:
+ * card-text-font-family:
+ * card-text-font-size:
+ * card-text-font-weight:
+ * card-text-line-height:
+ * card-border-width:
+ * card-border-style:
+ * card-border-color:
+ * card-border-radius:
+ * card-padding:
+ * card-shadow:
+ * card-divider-color:
